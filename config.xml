<?xml version='1.0' encoding='utf-8'?>
<widget id="com.benthegoose.explorer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Explorer</name>
    <description>
        An app which makes you do things that you usually do not do.
    </description>
    <author email="bendeguznak@gmail.com" href="https://benthegoose.com">
        Goose, who dies in every moovie
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="www/img/e-dark.png" target="res/drawable/e.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="22" />
    <plugin name="cordova-plugin-local-notification" spec="^0.8.4" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <engine name="android" spec="^6.2.3" />
</widget>
