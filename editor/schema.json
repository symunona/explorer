
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "string",
      "title": "The Id Schema ",
      "default": "",
      "examples": [
        "asdf"
      ]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": [
        "password"
      ]
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": [
        "Pöri célokat keres"
      ]
    },
    "description": {
      "$id": "/properties/description",
      "type": "string",
      "title": "The Description Schema ",
      "default": "",
      "examples": [
        "Todo: Mi ez, hogy működik, mit csinálj, mit ne csinálj, mire számíts, mit vigyél magaddal"
      ]
    },
    "rules": {
      "$id": "/properties/rules",
      "type": "object",
      "properties": {
        "date": {
          "$id": "/properties/rules/properties/date",
          "type": "object"
        },
        "time": {
          "$id": "/properties/rules/properties/time",
          "type": "object",
          "properties": {
            "after": {
              "$id": "/properties/rules/properties/time/properties/after",
              "type": "string",
              "title": "The After Schema ",
              "default": "",
              "examples": [
                "08:00"
              ]
            },
            "before": {
              "$id": "/properties/rules/properties/time/properties/before",
              "type": "string",
              "title": "The Before Schema ",
              "default": "",
              "examples": [
                "18:00"
              ]
            }
          }
        }
      }
    },
    "challenges": {
      "$id": "/properties/challenges",
      "type": "array",
      "items": {
        "$id": "/properties/challenges/items",
        "type": "object",
        "properties": {
          "id": {
            "$id": "/properties/challenges/items/properties/id",
            "type": "integer",
            "title": "The Id Schema ",
            "default": 0,
            "examples": [
              1
            ]
          },
          "title": {
            "$id": "/properties/challenges/items/properties/title",
            "type": "string",
            "title": "The Title Schema ",
            "default": "",
            "examples": [
              "Hello."
            ]
          },
          "description": {
            "$id": "/properties/challenges/items/properties/description",
            "type": "string",
            "title": "The Description Schema ",
            "default": "",
            "examples": [
              "Ez egy teszt uzenet"
            ]
          },
          "time": {
            "$id": "/properties/challenges/items/properties/time",
            "type": "string",
            "title": "The Time Schema ",
            "default": "",
            "examples": [
              "1"
            ]
          }
        }
      }
    }
  }
}
